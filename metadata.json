        "primaryUse": true
      },
      "txt": {
        "description": "QGIS color map export format",
        "mimeType": "text/plain",
        "standard": "QGIS",
        "primaryUse": false,
        "legacy": true
      }
    },
    "legend": {
      "png": {
        "description": "Legend image in PNG format",
        "mimeType": "image/png",
        "primaryUse": true
      }
    }
  },
